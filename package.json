{
	"name": "svelte-switcher",
	"version": "1.0.2",
	"scripts": {
		"dev": "npm run build:docs && vite dev",
		"build": "npm run build:docs && vite build && svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"pre:package-release": "rm -rf ./package",
		"package": "svelte-package && cp ./README.md ./package/README.md",
		"build:docs": "node setupExamples.js",
		"release": "release-it"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-vercel": "^1.0.0-next.81",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "^1.0.0-next.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.19",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"release-it": "^15.5.0",
		"shiki": "^0.11.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"sveltekit",
		"svelte-toggle",
		"svelte-toggler",
		"svelte-switch",
		"switch-component",
		"theme-switcher",
		"svelte-theme-switcher",
		"svelte-theme-toggle",
		"svelte-button",
		"accessibility",
		"customisable",
		"svelte-input",
		"svelte-toggle-input",
		"svelte-toggle-button"
	]
}
